selectClass
{
	selector
	{
		condition team == TEAM_ALIENS
		{
			spawnAs PCL_ALIEN_LEVEL0
		}
		condition team == TEAM_HUMANS
		{
			spawnAs WP_MACHINEGUN
		}
	}
}

selector
{
	behavior unstick

	condition ( team == TEAM_ALIENS )
	{
		behavior default_aliens
	}

	condition ( team == TEAM_HUMANS )
	{
		behavior default_humans
	}
}
