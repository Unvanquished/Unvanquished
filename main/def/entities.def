entityDef func_bobbing 
{   
	"editor_color"				"0 .5 .8"
	
	"editor_usage"				"Solid entity that oscillates back and forth in a linear motion. By default, it will have an amount of displacement in either direction equal to the dimension of the brush in the axis in which it's bobbing. Entity bobs on the Z axis (up-down) by default. It can also emit sound if the noise key is set. Will crush the player when blocked."

	"editor_float speed"		"Amount of time in seconds for one complete oscillation cycle."
	"editor_float height"		"Sets the amount of travel of the oscillation movement."
	"editor_float phase"		"Sets the start offset of the oscillation cycle. Values must be 0 &lt; phase &lt; 1. Any integer phase value is the same as no offset."
	"editor_var noise"			"Path/name of .wav file to play. Use looping sounds only."
	"editor_var dmg"			"Damage to inflict when blocked."
	"editor_bool x_axis"		"entity will bob along the X axis."
	"editor_bool y_axis"		"entity will bob along the Y axis."
	"editor_bool notfree"		"when set to 1, entity will not spawn in 'Free for all' and 'Tournament' modes."
	"editor_bool notteam"		"when set to 1, entity will not spawn in 'Teamplay' and 'CTF' modes."
	"editor_bool notsingle"		"when set to 1, entity will not spawn in Single Player mode (bot play mode)."

	"spawnclass"				"idSpawnableEntity"
    "speed"						"4"  
    "height"					"32"  
    "phase"						"0"  
    "noise"						""
    "dmg"						"4"
    "x_axis"					"0"
    "y_axis"					"0"
    "notfree"					"0"  
    "notteam"					"0"  
    "notsingle"                 "0"    
}


