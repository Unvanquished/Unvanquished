// Quarry Models



models/quarry/hanomag
{ 
qer_editorimage models/quarry/hanomag 
q3map_forceMeta 
q3map_nonplanar 
q3map_shadeAngle 180 
q3map_lightmapSampleOffset 8.0
implicitMap models/quarry/hanomag  
}

models/quarry/hanomag_dcull
{ 
qer_editorimage models/quarry/hanomag
cull twosided 
q3map_forceMeta 
q3map_nonplanar 
q3map_shadeAngle 180 
q3map_lightmapSampleOffset 8.0
implicitMap models/quarry/hanomag  
}

